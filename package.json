{
  "name": "cardboard-butler",
  "version": "2.0.0",
  "description": "Find the boardgames from BGG to play",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "sass",
    "bgg",
    "boardgames"
  ],
  "author": "Philip Kristoffersen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PhilipK/CardboardButler.git"
  },
  "bugs": {
    "url": "https://github.com/PhilipK/CardboardButler/issues"
  },
  "homepage": "https://github.com/PhilipK/CardboardButler",
  "scripts": {
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && rm -rf dist && webpack --mode production",
    "start": "yarn run start-dev",
    "start-dev": "export NODE_OPTIONS=--openssl-legacy-provider && webpack-dev-server --mode development",
    "start-prod": "yarn run build && node scripts/prod-server.js",
    "test-coverage": "jest --watch --watchAll --coverage --verbose",
    "test": "jest --watch",
    "test-ci": "jest --coverage --ci --reporters=default --reporters=jest-junit"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@hot-loader/react-dom": "^16.9.0",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.4",
    "@types/fetch-mock": "^7.3.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.4",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "eslint": "7.32.0",
    "eslint-plugin-react": "7.33.2",
    "express": "^4.17.1",
    "fetch-mock": "^7.3.9",
    "fetchmock": "^1.0.1",
    "file-loader": "^4.2.0",
    "fs": "^0.0.1-security",
    "html-webpack-plugin": "^3.2.0",
    "ignore-styles": "^5.0.1",
    "import-local": "^3.0.2",
    "jest": "^24.9.0",
    "jest-junit": "^8.0.0",
    "node-fetch": "^2.6.1",
    "react-hot-loader": "^4.12.12",
    "sass": "^1.97.2",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "typescript": "^3.6.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-middleware": "^3.7.1",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2",
    "whatwg-fetch": "^3.0.0",
    "yarn-upgrade-all": "^0.5.2"
  },
  "dependencies": {
    "fast-memoize": "^2.5.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "semantic-ui-react": "^0.88.0",
    "ts-jest": "^24.0.2",
    "xml-js": "^1.6.11"
  }
}
